{
    "main": "./node_modules/expo/AppEntry.js",
    "scripts": {
        "start": "expo start",
        "android": "expo start --android",
        "ios": "expo start --ios",
        "web": "expo start --web",
        "eject": "expo eject",
        "test": "jest --coverage --runInBand --detectOpenHandles",
        "test:pre-push": "CI=true jest --coverage --updateSnapshot",
        "prettier-watch": "onchange \"**/*\" -- prettier --write --ignore-unknown {{changed}}",
        "test:debug": "node --inspect-brk node_modules/jest/bin/jest.js --runInBand",
        "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.json",
        "lint-and-fix": "eslint . --ext .ts,.tsx,.js,.jsx,.json --fix",
        "prettier-format": "prettier --config .prettierrc.js '**/*.{json,js,jsx,ts,tsx,css,scss,md}' --write",
        "ci": "npm run lint && npm run test && sonar-scanner"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run prettier-format && npm run lint-and-fix",
            "pre-push": "npm run prettier-format && npm run lint-and-fix"
        }
    },
    "jest": {
        "preset": "jest-expo",
        "transformIgnorePatterns": [
            "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*)"
        ]
    },
    "dependencies": {
        "@expo/vector-icons": "^12.0.2",
        "@react-native-async-storage/async-storage": "^1.13.2",
        "@react-native-community/hooks": "^2.6.0",
        "@react-native-community/masked-view": "0.1.10",
        "@react-native-community/netinfo": "5.9.7",
        "@react-native-community/slider": "3.0.3",
        "@react-navigation/bottom-tabs": "^5.8.0",
        "@react-navigation/drawer": "^5.9.3",
        "@react-navigation/native": "^5.2.1",
        "@react-navigation/stack": "5.2.16",
        "@types/date-and-time": "^0.13.0",
        "axios": "^0.21.1",
        "date-and-time": "^0.14.1",
        "expo": "^40.0.0",
        "expo-asset": "~8.2.0",
        "expo-av": "~8.7.0",
        "expo-constants": "~9.3.3",
        "expo-font": "8.4.0",
        "expo-linking": "^2.0.0",
        "expo-status-bar": "~1.0.2",
        "expo-video-player": "^1.6.1",
        "expo-web-browser": "~8.6.0",
        "jsonfile": "^6.1.0",
        "lodash.clonedeep": "^4.5.0",
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "react-native": "^0.63.4",
        "react-native-animatable": "^1.3.3",
        "react-native-chart-kit": "^6.8.1",
        "react-native-check-box": "^2.1.7",
        "react-native-dotenv": "^2.4.3",
        "react-native-gesture-handler": "~1.8.0",
        "react-native-get-random-values": "^1.5.1",
        "react-native-paper": "^4.6.0",
        "react-native-plotly": "^4.0.0",
        "react-native-progress": "^4.1.2",
        "react-native-safe-area-context": "3.1.9",
        "react-native-screens": "~2.15.2",
        "react-native-svg": "^12.1.0",
        "react-native-vector-icons": "^7.1.0",
        "react-native-web": "~0.13.12",
        "react-native-webview": "11.0.0",
        "shortid": "^2.2.16",
        "telejson": "^5.1.0",
        "ts-jest": "^26.4.1",
        "uuid": "^8.3.2",
        "react-native-reanimated": "~1.13.0"
    },
    "devDependencies": {
        "@babel/core": "~7.9.0",
        "@testing-library/jest-native": "^3.4.3",
        "@testing-library/react-native": "^7.0.2",
        "@types/jest": "^26.0.14",
        "@types/react": "^16.14.2",
        "@types/react-native": "0.63.2",
        "@types/react-native-dotenv": "^0.2.0",
        "@types/react-test-renderer": "^16.9.3",
        "@types/serialize-javascript": "^5.0.0",
        "@types/shortid": "0.0.29",
        "@typescript-eslint/eslint-plugin": "^4.14.0",
        "@typescript-eslint/parser": "^4.14.0",
        "babel-core": "^7.0.0-bridge.0",
        "babel-jest": "^26.6.0",
        "eslint": "^7.17.0",
        "eslint-config-prettier": "^7.2.0",
        "eslint-config-react": "^1.1.7",
        "eslint-plugin-jest": "^24.1.3",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-react": "^7.22.0",
        "husky": "^4.3.8",
        "jest": "^26.6.3",
        "jest-expo": "^39.0.0",
        "jest-fetch-mock": "^3.0.3",
        "prettier": "^2.2.1",
        "react-test-renderer": "^16.14.0",
        "sonar-scanner": "^3.1.0",
        "typescript": "^4.1.3"
    },
    "private": true
}
