{
    "expo": {
        "name": "SkillSTAR",
        "slug": "SkillSTAR",
        "privacy": "unlisted",
        "version": "1.0.7",
        "orientation": "portrait",
        "icon": "./assets/images/app_icon.png",
        "scheme": "myapp",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./assets/images/splash_screen/splash_screen.png",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "buildNumber": "9",
            "supportsTablet": true,
            "bundleIdentifier": "SkillSTAR",
            "infoPlist": {
                "UIBackgroundModes": ["fetch", "remote-notification"],
                "ITSAppUsesNonExemptEncryption": "NO"
            }
        },
        "web": {
            "favicon": "./assets/images/favicon.png"
        },
        "description": "",
        "githubUrl": "https://github.com/sartography/skillstar",
        "android": {
            "package": "com.ajlouie.SkillSTAR"
        }
    }
}
